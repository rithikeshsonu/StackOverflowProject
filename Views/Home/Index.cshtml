@model List<StackOverflowProject.ViewModels.QuestionViewModel>


@{ 
    ViewBag.Title = "Home";
}
<div class="row">
    <div class="col-sm-12">
        <div class="jumbotron">
            <h1>Learn, Share, Build</h1>
            <h3>
                Lorem ipsum dolor sit amet consectetur adipisicing elit. Delectus iusto culpa nesciunt? Adipisci tempora nesciunt unde recusandae sed saepe amet ratione. Rerum corrupti quod officiis eligendi reprehenderit? At, tempora voluptatem.
            </h3>
            <p class="lead">oluptatum sed distinctio sunt provident quasi exercitationem incidunt offi</p>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-12">
            <table class="table">
                @foreach(var item in Model)
                {
                    Html.RenderPartial("Question", item);
                }
            </table>
            @if (Session["CurrentUserName"] != null)
            {
                <a href="/questions/create" class="btn btn-primary">Add question</a>
            }
            
        </div>

    </div>
    
</div>

<script>
    $("a[href='/home/index']"). attr("id", "active");
</script>